    "import string\n",
    "import random\n",
    "\n",
    "def id_generator(size, chars=string.ascii_uppercase):\n",
    "    return ''.join(random.choice(chars) for _ in range(size))\n",
    "\n",
    "N = 10\n",
    "M = 11\n",
    "\n",
    "print(id_generator(N))\n",
    "print(id_generator(M))"
